{
  "name": "bismarck-cs",
  "version": "0.0.22-beta.1",
  "description": "Computer Science Repository",
  "main": "src/index.js",
  "repository": "git@github.com:Oscarz90/bismarck-cs.git",
  "author": "Oscar Martinez Sanchez <omsz90@live.com.mx>",
  "license": "Apache-2.0",
  "scripts": {
    "benchmark": "babel-node benchmark",
    "benchmark:watch": "nodemon src/index.js --exec babel-node benchmark",
    "build": "cross-env NODE_ENV=production babel src -d dist -s",
    "changelog": "standard-changelog --preset angular --outfile changelog-temp.md",
    "clean": "yarn run clean:dist && yarn run clean:reports && yarn run clean:coverage",
    "clean:coverage": "shx rm -rf coverage",
    "clean:dist": "shx rm -rf dist",
    "clean:reports": "shx rm -rf reports",
    "coverage": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text mocha --recursive src",
    "debug": "nodemon src/index.js --exec babel-node --debug",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "lint:git": "commitlint -e",
    "lint:markdown": "markdownlint README.md",
    "lint:markdown-links": "markdown-link-check README.md",
    "monitor": "nodejs-dashboard node dist",
    "postcoverage": "cross-env NODE_ENV=test nyc check-coverage",
    "posttest": "cross-env NODE_ENV=test mocha -S -R mochawesome --require @babel/register --reporter-options reportDir=reports --check-leaks --recursive src",
    "prebuild": "yarn run clean:dist",
    "precoverage": "yarn run clean:coverage",
    "premonitor": "yarn run clean:dist && yarn run build",
    "prerelease": "yarn run setenv:prod && yarn run clean && yarn run build && yarn run lint && yarn run test && yarn run coverage",
    "pretest": "yarn run clean:coverage && yarn run clean:reports",
    "release": "standard-version",
    "publis:prerelease": "npm publish --tag latest",
    "security:auth": "snyk auth",
    "security:test": "snyk test",
    "security:wizard": "snyk wizard",
    "security:protect": "snyk protect",
    "security:monitor": "snyk monitor",
    "serve": "node dist",
    "setenv:dev": "yarn config set production false",
    "setenv:prod": "yarn config set production true",
    "start": "nodemon --exec babel-node src/index.js",
    "tasks": "ntl",
    "test": "cross-env NODE_ENV=test mocha -c -S -R spec --require @babel/register --check-leaks --recursive ./src/**/*.test.js",
    "test:single": "cross-env NODE_ENV=test mocha -w -c -S -R spec --require @babel/register --check-leaks",
    "test:watch": "cross-env NODE_ENV=test yarn test -w"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/*.{js}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.0",
    "@babel/register": "^7.9.0",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-angular": "^8.3.4",
    "@nx/cypress": "18.0.5",
    "@nx/eslint": "18.0.5",
    "@nx/jest": "18.0.5",
    "@nx/js": "^18.0.5",
    "@nx/react": "^18.0.5",
    "@nx/vite": "^18.0.5",
    "@nx/web": "18.0.5",
    "@swc-node/register": "~1.8.0",
    "@swc/cli": "~0.1.62",
    "@swc/core": "~1.3.85",
    "@swc/helpers": "~0.5.2",
    "@testing-library/react": "14.0.0",
    "@types/jest": "^29.4.0",
    "@types/node": "18.16.9",
    "@types/react": "18.2.33",
    "@types/react-dom": "18.2.14",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/coverage-v8": "^1.0.4",
    "@vitest/ui": "^1.3.1",
    "babel-eslint": "^10.1.0",
    "babel-plugin-istanbul": "^6.0.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "benchmark": "^2.1.4",
    "chai": "^4.2.0",
    "cross-env": "^7.0.2",
    "cypress": "13.6.4",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-import-resolver-babel-module": "^5.1.2",
    "eslint-plugin-cypress": "^2.13.4",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-mocha": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "^4.2.3",
    "jest": "^29.4.1",
    "jest-environment-node": "^29.4.1",
    "jsdom": "~22.1.0",
    "lint-staged": "^10.1.1",
    "markdown-link-check": "^3.8.1",
    "markdownlint-cli": "^0.22.0",
    "mocha": "^7.1.1",
    "mochawesome": "^5.0.0",
    "nodejs-dashboard": "^0.5.1",
    "nodemon": "^2.0.2",
    "ntl": "^5.0.0",
    "nx": "18.0.5",
    "nyc": "^15.0.0",
    "prettier": "^2.6.2",
    "sass": "^1.55.0",
    "shx": "^0.3.2",
    "snyk": "^1.305.1",
    "standard-changelog": "^2.0.21",
    "standard-version": "^7.1.0",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "typescript": "~5.3.2",
    "verdaccio": "^5.0.4",
    "vite": "~5.0.0",
    "vitest": "^1.3.1"
  },
  "dependencies": {
    "lodash": "^4.17.15",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.11.2",
    "tslib": "^2.3.0"
  },
  "nx": {
    "includedScripts": []
  }
}
