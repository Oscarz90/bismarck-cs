{
  "name": "bismarck-cs",
  "version": "0.0.15",
  "description": "Computer Science Repository",
  "main": "src/index.js",
  "repository": "git@github.com:Oscarz90/bismarck-cs.git",
  "author": "Oscar Martinez Sanchez <omsz90@live.com.mx>",
  "license": "Apache-2.0",
  "scripts": {
    "benchmark": "babel-node benchmark",
    "benchmark:watch": "nodemon src/index.js --exec babel-node benchmark",
    "build": "cross-env NODE_ENV=production babel src -d dist -s",
    "clean": "yarn run clean:dist && yarn run clean:reports && yarn run clean:coverage",
    "clean:coverage": "shx rm -rf coverage",
    "clean:dist": "shx rm -rf dist",
    "clean:reports": "shx rm -rf reports",
    "coverage": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text mocha src/*.js",
    "debug": "nodemon src/index.js --exec babel-node --debug",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "monitor": "nodejs-dashboard node dist",
    "postcoverage": "cross-env NODE_ENV=test nyc check-coverage",
    "posttest": "cross-env NODE_ENV=test mocha -S -R mochawesome --require @babel/register --reporter-options reportDir=reports --check-leaks ./src/**/*.test.js",
    "prebuild": "yarn run clean:dist",
    "precoverage": "yarn run clean:coverage",
    "premonitor": "yarn run clean:dist && yarn run build",
    "serve": "node dist",
    "start": "nodemon --exec babel-node src/index.js",
    "pretest": "yarn run clean:coverage && yarn run clean:reports",
    "test": "cross-env NODE_ENV=test mocha -c -S -R spec --require @babel/register --check-leaks ./src/**/*.test.js",
    "test:single": "cross-env NODE_ENV=test mocha -c -S -R spec --require @babel/register --check-leaks",
    "test:watch": "cross-env NODE_ENV=test yarn test -w"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.0",
    "@babel/register": "^7.9.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-istanbul": "^6.0.0",
    "benchmark": "^2.1.4",
    "chai": "^4.2.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-mocha": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.3",
    "lint-staged": "^10.1.1",
    "mocha": "^7.1.1",
    "mochawesome": "^5.0.0",
    "nodejs-dashboard": "^0.5.1",
    "nodemon": "^2.0.2",
    "nyc": "^15.0.0",
    "prettier": "^2.0.2",
    "shx": "^0.3.2"
  }
}
